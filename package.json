{
  "name": "@jbro/tibetan-transliterator",
  "version": "0.8.0",
  "description": "A naive attempt at automatically generating reliable Tibetan transliterations based on customizable sets of rules.",
  "main": "dist/tibetan-transliterator.umd.js",
  "dependencies": {
    "tibetan-normalizer": "^0.5.0",
    "tibetan-syllable-parser": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "acorn": "^7.4.1",
    "rollup": "^1.32.1",
    "jest": "^26.6.0",
    "jest-cli": "^26.6.0",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-serializer-html": "^7.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist && mkdir dist",
    "build": "npm run clean && npm run build:esm && npm run build:umd",
    "build:esm": "rollup src/transliterator.js --format esm --name TibetanTransliterator --file dist/tibetan-transliterator.esm.js",
    "build:umd": "rollup src/transliterator.js --format umd --name TibetanTransliterator --file dist/tibetan-transliterator.umd.js",
    "test": "jest --colors"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
